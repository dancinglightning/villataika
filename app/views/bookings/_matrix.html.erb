<table>
  <tr>
    <td> <a href="new?start=<%=start - 30.day%>"> month < </a> </td>
    <td> <a href="new?start=<%=start - 7.day%>"> week < </a> </td>
    <td> <%=start%> -- <%=start + 10.day%></td>
    <td> <a href="new?start=<%=start + 7.day%>"> week > </a> </td>
    <td> <a href="new?start=<%=start + 30.day%>"> month > </a> </td>
  </tr>
</tr>
<table>
  <thead>
    <th> </th>
    <% days.each do |day| %>
    <th>
      <%= day %>
    </th>
    <%end%>
  </thead>
  
  <tbody>
    <% rooms.each do |room| %>
    <tr>
      <td>
        <%= room %>
      </td>
      <% days.each do |day| %>
      <th>
        <%==div(room,day)%>
        <% if room_free(room,day) %>
        <a href="#">
          <%= image_tag "ok.jpeg"  , :height => "32px" , :width => "32px" %>
          </a>
        <%else%>
          <%= image_tag "not.jpeg"  , :height => "32px" , :width => "32px"%>
        <%end%>
      </th>
      <%end%>
    </tr>
    <%end%>
  </tbody>
</table>

<div>
<div id="room"> Choose </div>
<div id="arriving"> date </div>
</div>